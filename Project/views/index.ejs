        <!-------------------FEED-1---------------------->
        <% for (let i = 0; i < imageUrls.length; i++) { %>
        <div class="feeds">
            <div class="feed">
            <div class="head">
                <div class="user">
                    <div class="profile">
                        <img src="\images\profile.png.jpg">
                    </div>
                    <div class="ingo">
                        <h3>virat kohli</h3>
                        <small>Delhi, 10 MINUTES AGO</small>
                    </div> 
                </div>
                <span class="edit" >    
                        <button style="background-color: white;"><i class="uil uil-ellipsis-h"></i></button>
                </span>
            </div>  

            <!-- <img src="\images\feed\virat.webp" alt=""> -->
            
                <div class="photo">
                      <img src="<%= imageUrls[i] %>" alt="Image <%= i %>">
                    </div>  
                  

            <div class="action-button">
                <div class="interaction-button">
                <span><i class="uil uil-heart"></i></span>
                <span><i class="uil uil-comment-dots"></i></span>
                <span><i class="uil uil-share-alt"></i></span>
                </div>
